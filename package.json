{
  "name": "yfinance-ts",
  "version": "0.2.0",
  "description": "TypeScript port of yfinance - Download market data from Yahoo Finance API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:integration": "jest --config jest.integration.config.js",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "yahoo",
    "finance",
    "stocks",
    "market-data",
    "typescript"
  ],
  "author": "Rodrigo Vargas",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yfinance-ts/yfinance-ts.git"
  },
  "bugs": {
    "url": "https://github.com/yfinance-ts/yfinance-ts/issues"
  },
  "homepage": "https://github.com/yfinance-ts/yfinance-ts#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/luxon": "^3.3.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cheerio": "^1.0.0",
    "curl-cffi": "^0.1.41",
    "luxon": "^3.4.0",
    "mathjs": "^12.0.0",
    "ws": "^8.14.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
